{
    "name": "ticktock",
    "version": "0.0.0",
    "private": true,
    "description": "TickTock-JS is a timekeeping library for animating and adding interactions to digital watch designs.",
    "main": "index.js",
    "packageManager": "yarn@3.1.1",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "demo": "yarn workspace demo",
        "ticktock": "yarn workspace ticktock-js",
        "lint": "eslint",
        "lint:fix": "yarn lint -- --fix",
        "prettier": "prettier ./ --check",
        "prettier:fix": "yarn prettier -- --write",
        "format": "yarn prettier:fix && yarn lint:fix"
    },
    "author": "Daniel Yuschick <daniel.yuschick@gmail.com>",
    "license": "UNLICENSED",
    "keywords": [
        "ticktock",
        "svg",
        "animation",
        "ticktock-js",
        "watchmaking"
    ],
    "devDependencies": {},
    "dependencies": {}
}
