{
    "name": "ticktock-js",
    "version": "2.13.0",
    "description": "TickTock is a JavaScript library used to animate mechanical watches and clocks.",
    "main": "./dist/index.js",
    "scripts": {
        "watch": "webpack -w",
        "prebuild": "npm-run-all test docs",
        "build": "babel ./src/modules/ --out-dir ./dist/modules/ --copy-files --presets=es2015,react",
        "postbuild": "babel ./src/index.js --out-file ./dist/index.js --presets=es2015,react",
        "docs": "docco -o ./docs/ ./src/**/*.js",
        "test": "mocha"
    },
    "author": "Daniel Yuschick",
    "license": "No License",
    "devDependencies": {
        "autoprefixer": "^7.1.1",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.21.0",
        "babel-loader": "^6.2.10",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "browser-sync": "^2.18.6",
        "browser-sync-webpack-plugin": "^1.1.3",
        "chai": "^4.1.2",
        "docco": "^0.7.0",
        "mocha": "^3.4.2",
        "webpack": "^1.14.0",
        "yarn-run-all": "^3.1.1"
    },
    "dependencies": {
        "json-loader": "^0.5.7",
        "moment": "^2.18.1",
        "moment-timezone": "^0.5.13"
    },
    "browserslist": [
        "last 3 versions"
    ],
    "keywords": [
        "TickTock",
        "JavaScript",
        "SVG",
        "Animation",
        "Watch",
        "Clock",
        "Tick",
        "Tock"
    ],
    "engines": {
        "node": ">=4.2.4"
    }
}
