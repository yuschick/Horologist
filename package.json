{
  "name": "ticktock-js",
  "version": "2.9.1",
  "description": "TickTock is a JavaScript library used to animate mechanical watches and clocks.",
  "main": "index.js",
  "scripts": {
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "postcss -u autoprefixer --no-map -r ./src/styles/app.css -o ./dist/styles/app.css",
    "watch:scripts": "webpack -w",
    "prebuild": "npm run test",
    "build": "npm-run-all --parallel build:*",
    "build:styles": "postcss -u autoprefixer --no-map -r ./dist/styles/*.css",
    "build:docs": "docco -o ./docs/ ./src/**/*.js",
    "build:minifycss": "minify ./dist/styles/app.css",
    "build:minifyjs": "minify ./dist/js/app.js",
    "build:modules": "babel ./src/modules/*.js -d ./dist/ --presets=es2015,react",
    "postbuild": "babel ./src/index.js --out-file ./index.js --presets=es2015,react",
    "test": "mocha"
  },
  "author": "Daniel Yuschick",
  "license": "No License",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^4.0.1",
    "docco": "^0.7.0",
    "minifier": "^0.8.1",
    "mocha": "^3.4.2",
    "postcss-cli": "^4.0.0",
    "webpack": "^1.14.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "json-loader": "^0.5.7",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "keywords": [
    "TickTock",
    "JavaScript",
    "SVG",
    "Animation",
    "Watch",
    "Clock",
    "Tick",
    "Tock"
  ],
  "engines": {
    "node": ">=4.2.4"
  }
}
