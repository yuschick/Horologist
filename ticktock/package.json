{
  "name": "ticktock-js-site",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.5.1",
    "postcss-cli": "^4.0.0",
    "prettier": "^1.3.1",
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "ticktock-js": "^2.4.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "scripts": {
    "post-css": "postcss -u autoprefixer --no-map ./src/App.css",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run post-css && react-scripts build",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "precommit": "lint-staged",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    "last 3 versions"
  ]
}
